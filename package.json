{
	"name": "quantumpie.net",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && echo 'quantumpie.net' > ./build/CNAME",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"format:data": "prettier --plugin-search-dir ./src/lib/ --write ./data/steam_raw.ts ./data/genshin_raw.ts ./src/lib/steam_agg.ts",
		"deploy": "pnpm build && pnpx gh-pages -d build -t true",
		"prefetch": "tsx scripts/prefetch_steam.ts && tsx scripts/prefetch_genshin.ts",
		"aggregate": "tsx scripts/optimize_steam.ts && tsx scripts/optimize_genshin.ts",
		"screenshots": "tsx scripts/optimize_screenshots.ts"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.382",
		"@skeletonlabs/skeleton": "^3.2.0",
		"@skeletonlabs/skeleton-svelte": "^1.5.1",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/enhanced-img": "^0.4.4",
		"@sveltejs/kit": "^2.37.1",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@tailwindcss/forms": "^0.5.10",
		"@types/node": "^22.18.1",
		"@typescript-eslint/eslint-plugin": "^8.43.0",
		"@typescript-eslint/parser": "^8.43.0",
		"akasha-system.js": "^0.3.0",
		"enka-network-api": "^5.0.9",
		"eslint": "^9.35.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^2.46.1",
		"gh-pages": "^6.3.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"sharp": "^0.34.3",
		"svelte": "^5.38.7",
		"svelte-check": "^4.3.1",
		"tailwindcss": "^4.1.13",
		"ts-node": "^10.9.2",
		"tslib": "^2.8.1",
		"tsx": "^4.20.5",
		"typescript": "^5.9.2",
		"unplugin-icons": "^22.2.0",
		"vite": "^6.3.6",
		"vite-imagetools": "^7.1.1"
	},
	"type": "module",
	"dependencies": {
		"@tailwindcss/vite": "^4.1.13",
		"dompurify": "^3.2.6",
		"marked": "^15.0.12",
		"puppeteer": "^24.19.0",
		"puppeteer-extra": "^3.3.6",
		"puppeteer-extra-plugin-stealth": "^2.11.2"
	}
}
