{
	"name": "quantumpie.net",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && echo 'quantumpie.net' > ./build/CNAME",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"format:data": "prettier --plugin-search-dir ./src/lib/ --write ./data/steam_raw.ts ./data/genshin_raw.ts ./src/lib/steam_agg.ts",
		"deploy": "pnpm build && pnpx gh-pages -d build -t true",
		"prefetch": "tsx scripts/prefetch_steam.ts && tsx scripts/prefetch_genshin.ts",
		"aggregate": "tsx scripts/optimize_steam.ts && tsx scripts/optimize_genshin.ts"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.300",
		"@skeletonlabs/skeleton": "^2.11.0",
		"@skeletonlabs/tw-plugin": "^0.4.1",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/enhanced-img": "^0.4.4",
		"@sveltejs/kit": "^2.16.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/node": "^22.12.0",
		"@typescript-eslint/eslint-plugin": "^8.22.0",
		"@typescript-eslint/parser": "^8.22.0",
		"akasha-system.js": "^0.3.0",
		"axios": "^1.7.9",
		"enka-network-api": "^4.6.4",
		"eslint": "^9.19.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"gh-pages": "^6.3.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.19.4",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^4.0.12",
		"ts-node": "^10.9.2",
		"tslib": "^2.8.1",
		"tsx": "^4.19.2",
		"typescript": "^5.7.3",
		"unplugin-icons": "^22.0.0",
		"vite": "^6.0.11",
		"vite-imagetools": "^7.0.5"
	},
	"type": "module",
	"dependencies": {
		"@tailwindcss/vite": "^4.0.12"
	}
}