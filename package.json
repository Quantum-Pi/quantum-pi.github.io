{
	"name": "quantumpie.net",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && echo 'quantumpie.net' > ./build/CNAME",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"format:data": "prettier --plugin-search-dir ./src/lib/ --write ./data/steam_raw.ts ./data/genshin_raw.ts ./src/lib/steam_agg.ts",
		"deploy": "pnpm build && pnpx gh-pages -d build -t true",
		"prefetch": "tsx scripts/prefetch_steam.ts && tsx scripts/prefetch_genshin.ts",
		"aggregate": "tsx scripts/optimize_steam.ts && tsx scripts/optimize_genshin.ts",
		"screenshots": "tsx scripts/optimize_screenshots.ts"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.425",
		"@skeletonlabs/skeleton": "^3.2.2",
		"@skeletonlabs/skeleton-svelte": "^1.5.3",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/enhanced-img": "^0.4.4",
		"@sveltejs/kit": "^2.49.3",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@tailwindcss/forms": "^0.5.11",
		"@types/node": "^22.19.3",
		"@typescript-eslint/eslint-plugin": "^8.52.0",
		"@typescript-eslint/parser": "^8.52.0",
		"akasha-system.js": "^0.3.0",
		"enka-network-api": "latest",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^2.46.1",
		"gh-pages": "^6.3.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"sharp": "^0.34.5",
		"svelte": "^5.46.1",
		"svelte-check": "^4.3.5",
		"tailwindcss": "^4.1.18",
		"ts-node": "^10.9.2",
		"tslib": "^2.8.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"unplugin-icons": "^22.5.0",
		"vite": "^6.4.1",
		"vite-imagetools": "^7.1.1"
	},
	"type": "module",
	"dependencies": {
		"@tailwindcss/vite": "^4.1.18",
		"dompurify": "^3.3.1",
		"marked": "^15.0.12",
		"puppeteer": "^24.34.0",
		"puppeteer-extra": "^3.3.6",
		"puppeteer-extra-plugin-stealth": "^2.11.2"
	}
}
