{
	"name": "quantumpie.net",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && echo 'quantumpie.net' > ./build/CNAME",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"format:profile": "prettier --plugin-search-dir ./src/lib/ --write ./src/lib/profile.ts",
		"deploy": "pnpm build && pnpx gh-pages -d build -t true",
		"prefetch": "tsx scripts/prefetch.ts"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.129",
		"@savvywombat/tailwindcss-grid-areas": "^3.1.0",
		"@skeletonlabs/skeleton": "^2.3.0",
		"@skeletonlabs/tw-plugin": "^0.2.2",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/adapter-static": "^3.0.2",
		"@sveltejs/enhanced-img": "^0.3.0",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/node": "^20.8.5",
		"@typescript-eslint/eslint-plugin": "^6.7.5",
		"@typescript-eslint/parser": "^6.7.5",
		"autoprefixer": "^10.4.16",
		"axios": "^1.7.2",
		"eslint": "^8.51.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.34.0",
		"gh-pages": "^6.0.0",
		"postcss": "^8.4.31",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.2.1",
		"svelte-check": "^3.5.2",
		"tailwindcss": "^3.3.3",
		"ts-node": "^10.9.2",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"unplugin-icons": "^0.17.0",
		"vite": "^5.3.4",
		"vite-imagetools": "^7.0.4"
	},
	"type": "module"
}